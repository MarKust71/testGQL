    "url": "https://github.com/CaptainObjective/JuniorQuest/issues"
  },
  "homepage": "https://github.com/CaptainObjective/JuniorQuest#readme",
  "devDependencies": {
    "concurrently": "^5.0.0",
    "eslint": "^6.5.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.1.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {}
